// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["orderByNulls"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./kaizoku.db"
}

model Manga {
  id       Int    @id @default(autoincrement())
  title    String
  cover    String
  interval String
  source   String
  query    String
}
